{
    "version": 2,
    "rewrites": [
        {
            "source": "/probe-root",
            "destination": "https://google.com"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 300
        }
    }
}