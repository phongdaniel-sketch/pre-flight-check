{
    "version": 2,

    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/index.py"
        },
        {
            "src": "/docs",
            "dest": "api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "api/index.py"
        },
        {
            "src": "/",
            "dest": "/static/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        }
    ]
}